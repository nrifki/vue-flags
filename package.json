{
  "private": false,
  "name": "vue-flags",
  "description": "An extremely lightweight Vue.js library of flags from all over the world. 🌍",
  "author": "Nada Rifki",
  "license": "MIT",
  "version": "0.0.4",
  "main": "./dist/index.cjs.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "bili --vue.css false",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "rollup-plugin-copy": "^0.2.3",
    "vue": "^2.5.21"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-service": "^3.3.0",
    "babel-eslint": "^10.0.1",
    "bili": "^4.0.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.11.0",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "rollup-plugin-copy-assets": "^1.1.0",
    "rollup-plugin-vue": "^4.6.1",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
  },
  "files": [
    "dist/*"
  ],
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
